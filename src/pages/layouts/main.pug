include ../mixins/meta

doctype html

html
  head
    block meta
      +meta()
    block stylesheets
      link(rel='stylesheet', href='./styles/index.scss')
      link(rel="icon", href="./assets/favicon.png")
    block head-additions

  body.wds
    header
      block header
        svg.logo
          use(xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="./assets/images/logo.svg#logo")
    main
      block content
    footer
      block footer

    block javascripts
      script(src='./scripts/index.js')
